irs(tbl) do\r\n        table.insert(data, core:new_search(\r\n                v[\"name\"],\r\n                v[\"id\"],\r\n                \"http://img.webnovel.com/bookcover/\" .. v[\"id\"] .. \"/150/150.jpg\",\r\n                \"https://www.webnovel.com/book/\")\r\n        )\r\n    end\r\n    list[\"data\"] \u003d data\r\n\r\nend\r\nreturn list\r\n","detail":"local url \u003d ...\r\n\r\nlocal bookId \u003d regexp:find(url, \"/book/(\\\\d+)/?\")\r\nlocal doc \u003d http:request(\"https://www.webnovel.com/book/\" .. bookId):cookie(false, true):html()\r\n\r\nif doc ~\u003d nil then\r\n    local book \u003d {}\r\n    book[\"url\"] \u003d \"https://www.webnovel.com/book/\" .. bookId\r\n    local name \u003d doc:select(\"h2\"):first()\r\n    html:remove(name, { \"small\" })\r\n    book[\"name\"] \u003d name:text()\r\n    book[\"cover\"] \u003d core:merge_url(\"http:\", doc:select(\"i.g_thumb img\"):first():attr(\"src\"))\r\n    book[\"author\"] \u003d doc:select(\"address\"):last():select(\"span\"):first():text()\r\n    book[\"description\"] \u003d doc:select(\"div#about div.det-abt\"):html()\r\n    book[\"detail\"] \u003d doc:select(\"address\"):last():html()\r\n    return book\r\nend\r\nreturn nil","checked":true}