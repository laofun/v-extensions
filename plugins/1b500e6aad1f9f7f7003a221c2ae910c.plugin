{"uuid":"1b500e6aad1f9f7f7003a221c2ae910c","name":"Truyá»‡n CV","version":1.3,"url":"","language":"vi","icon":"","source":"http://truyencv.com","regex":"(www.)?truyencv.com/[^/]+/?$","author":"Darkrai","describe":"","group":"convert","supportUpdate":false,"chap":"local url \u003d ...\r\nlocal doc \u003d http:request(url):html()\r\nif doc ~\u003d nil then\r\n    local element \u003d doc:select(\"div.content\"):first()\r\n    if (string.len(element:text()) \u003c 2000) then\r\n        if (text:contains(element:html(), \"modal-login\")) then\r\n            return core:create_login(\"http://truyencv.com/login\")\r\n        else\r\n            return core:create_confirm(url)\r\n        end\r\n    end\r\n\r\n    html:remove(element, { \"div\", \"script\", \"font\", \"center\", \"button\", \"a\" })\r\n    if (element:select(\"p\"):size() \u003c 20) then\r\n        element:select(\"p\"):remove()\r\n    end\r\n\r\n    local title \u003d doc:select(\"div.header h2.title\"):first():text()\r\n\r\n    return core:new_chap(title, element:html())\r\nend\r\nreturn nil","toc":"local url \u003d ...\r\n\r\nlocal list \u003d {}\r\nlocal doc \u003d http:request(url):html()\r\nif doc ~\u003d nil then\r\n    local hash \u003d regexp:find(doc:select(\"div.truyencv-detail-tab\"):html(), \"showChapter\\\\((.*?)\\\\)\")\r\n    if (text:is_empty(hash)) then\r\n        return list\r\n    end\r\n    hash \u003d text:remove(hash, { \"\u0027\" })\r\n    local tbl \u003d text:split(hash, \",\")\r\n\r\n    local params \u003d { [\"showChapter\"] \u003d 1, [\"media_id\"] \u003d tbl[1], [\"number\"] \u003d 1, [\"page\"] \u003d 1, [\"type\"] \u003d tbl[4] }\r\n\r\n    doc \u003d http:request(\"https://truyencv.com/index.php\"):params(params):post():html()\r\n    local el \u003d doc:select(\"div.item a\")\r\n    html:remove(el, { \"span.text-muted\" })\r\n\r\n    if (doc:select(\"div.panel-vip\"):isEmpty()) then\r\n        for i \u003d el:size() - 1, 0, -1 do\r\n            local e \u003d el:get(i)\r\n            table.insert(list, core:new_chapter(e:text(), e:attr(\"href\"), \"http://truyencv.com\"))\r\n        end\r\n    else\r\n        for i \u003d 0, el:size() - 1 do\r\n            local e \u003d el:get(i)\r\n            table.insert(list, core:new_chapter(e:text(), e:attr(\"href\"), \"http://truyencv.com\"))\r\n        end\r\n    end\r\nend\r\nreturn list","page":"local url \u003d ...\r\n\r\nlocal list \u003d {}\r\ntable.insert(list, url)\r\nreturn list","search":"local key, page \u003d ...\r\nlocal doc \u003d http:request(\"http://truyencv.com/tim-kiem/all/\" .. key .. \"/trang-\" .. page + 1):html()\r\n\r\nlocal list \u003d {}\r\n\r\nif doc ~\u003d nil then\r\n\r\n    list[\"count\"] \u003d num:to_int(regexp:find_last(doc:select(\"ul.pagination li\"):last():html(), \"/trang-(\\\\d+)\"), 1)\r\n\r\n    local el \u003d doc:select(\"div.block-content ul.list-group li\")\r\n\r\n    if el ~\u003d nil then\r\n        local data \u003d {}\r\n        for i \u003d 0, el:size() - 1 do\r\n            local e \u003d el:get(i)\r\n            table.insert(data, core:new_search(\r\n                    e:select(\"h2.title a\"):text(),\r\n                    e:select(\"h2.title a\"):attr(\"href\"),\r\n                    e:select(\"a.thumb img\"):attr(\"src\"),\r\n                    \"http://truyencv.com\")\r\n            )\r\n        end\r\n        list[\"data\"] \u003d data\r\n    end\r\nend\r\nreturn list\r\n","detail":"local url \u003d ...\r\n\r\nlocal doc \u003d http:request(url):html()\r\n\r\nif doc ~\u003d nil then\r\n    local book \u003d {}\r\n    book[\"name\"] \u003d doc:select(\"h1.title\"):text()\r\n    book[\"cover\"] \u003d core:merge_url(\"http://truyencv.com\", doc:select(\"div.thumb img.img-responsive\"):attr(\"src\"))\r\n    book[\"author\"] \u003d doc:select(\"div.info div.item a\"):first():text()\r\n    book[\"description\"] \u003d doc:select(\"div.brief\"):html()\r\n    local detail \u003d \"\"\r\n    local el \u003d doc:select(\"div.info div.list div.item\")\r\n    for i \u003d 0, el:size() - 1 do\r\n        detail \u003d detail .. el:get(i):text() .. \"\u003cbr\u003e\"\r\n    end\r\n\r\n    book[\"detail\"] \u003d detail\r\n    return book\r\nend\r\nreturn nil","checked":true}