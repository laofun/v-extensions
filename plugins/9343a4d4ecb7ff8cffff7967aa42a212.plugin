{"uuid":"9343a4d4ecb7ff8cffff7967aa42a212","name":"MangaK","version":1.0,"url":"","language":"vi","icon":"\u003d\u003d","source":"http://mangak.info","regex":"(www.)?mangak.info/[^/]+/?$","author":"Darkrai","describe":"","group":"truyentranh","supportUpdate":false,"chap":"local url \u003d ...\r\nlocal doc \u003d http:request(url):html()\r\nif doc ~\u003d nil then\r\n    local txt \u003d doc:select(\"div.vung_doc script\"):first():toString()\r\n    if txt ~\u003d nil then\r\n        local json \u003d text:sub(txt, text:index_of(txt, \"[\"), text:last_index_of(txt, \"]\") + 1)\r\n        local img \u003d text:json_table(json)\r\n        local htm \u003d \"\"\r\n        if img ~\u003d nil then\r\n            for i, v in ipairs(img) do\r\n                htm \u003d htm .. \"\u003cimg src\u003d\\\"\" .. v .. \"\\\"\u003e\u003cbr\u003e\"\r\n            end\r\n        end\r\n        return htm\r\n    end\r\nend\r\nreturn nil","toc":"local url \u003d ...\r\n\r\nlocal list \u003d {}\r\nlocal doc \u003d http:request(url):html()\r\nif doc ~\u003d nil then\r\n    local el \u003d doc:select(\"div.chapter-list div.row a\")\r\n    for i \u003d el:size() - 1, 0, -1 do\r\n        local e \u003d el:get(i)\r\n        table.insert(list, core:new_chapter(e:text(), e:attr(\"href\"), \"http://mangak.info\"))\r\n    end\r\nend\r\nreturn list","search":"local key, page \u003d ...\r\nlocal doc \u003d http:request(\"http://mangak.info/page/\" .. page + 1 .. \"/?s\u003d\" .. key .. \"\u0026q\u003d\" .. key):html()\r\n\r\nlocal list \u003d {}\r\n\r\nif doc ~\u003d nil then\r\n\r\n    list[\"count\"] \u003d num:to_int(regexp:find(doc:select(\"div.wp-pagenavi a\"):last():attr(\"href\"), \"/page/(\\\\d+)/\"), 1)\r\n\r\n    local el \u003d doc:select(\"div.danh_sach div.update_item\")\r\n\r\n    if el ~\u003d nil then\r\n        local data \u003d {}\r\n        for i \u003d 0, el:size() - 1 do\r\n            local e \u003d el:get(i)\r\n            table.insert(data, core:new_search(\r\n                    e:select(\"h3 a\"):text(),\r\n                    e:select(\"h3 a\"):attr(\"href\"),\r\n                    e:select(\"img\"):first():attr(\"src\"),\r\n                    \"http://mangak.info\")\r\n            )\r\n        end\r\n        list[\"data\"] \u003d data\r\n    end\r\nend\r\nreturn list\r\n","detail":"local url \u003d ...\r\n\r\nlocal doc \u003d http:request(url):html()\r\n\r\nif doc ~\u003d nil then\r\n    local book \u003d {}\r\n    print(doc)\r\n    book[\"name\"] \u003d  doc:select(\"h1.entry-title\"):text()\r\n    book[\"cover\"] \u003d core:merge_url(\"https://mangak.info\", doc:select(\".info_image img\"):first():attr(\"src\"))\r\n    book[\"detail\"] \u003d doc:select(\"ul.truyen_info_right\"):html()\r\n    book[\"description\"] \u003d doc:select(\"meta[property\u003dog:description]\"):attr(\"content\")\r\n    return book\r\nend\r\nreturn nil","checked":true}